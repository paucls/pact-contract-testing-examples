{
  "provider": {
    "name": "catalog"
  },
  "consumer": {
    "name": "frontend"
  },
  "interactions": [
    {
      "description": "a request to favourite a product",
      "request": {
        "method": "POST",
        "path": "/products/10/favourite",
        "matchingRules": {
          "path": {
            "matchers": [
              {
                "match": "regex",
                "regex": "/products/10/favourite"
              }
            ],
            "combine": "AND"
          }
        }
      },
      "response": {
        "status": 204
      },
      "providerStates": [
        {
          "name": "product with id 10 exists"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.0"
    }
  }
}